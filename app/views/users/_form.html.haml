-form_for @user, :html => { :multipart => true } do |f|
  %p
    = f.label :user_id
    %br
    =# f.collection_select :user_id, User.all, :id, :name 
  %p
    =# f.label :name
    %br
    =# f.text_field :name 
  %p
    =# f.label :price 
    %br
    =# f.text_field :price
  %p
    =# f.label :description 
    %br
    =# f.text_area :description, :rows => 10 
  %p
    = f.file_field :mugshot
  %p
    = f.submit "Submit"
